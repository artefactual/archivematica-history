<extension>
  <parent></parent>
  <accessFormat>OGF</accessFormat>
  <preservationFormat>MXF</preservationFormat>
  <accessConversionCommand>%ffmpegPath% -i %fileFullName% -acodecvorbis %accessPath%%fileName%.%accessFormat%</accessConversionCommand>
