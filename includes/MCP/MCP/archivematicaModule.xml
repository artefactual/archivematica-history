<module>
    <name> </name>
    <description> </description>
    <watchFolder></watchFolder>
    <requireUsrApproval>Yes/No</requireUsrApproval>
    <folderToMoveToWhileProcessing></folderToMoveToWhileProcessing>
    <command>
        <forceLocal>Yes/No</forceLocal>
        <ExecuteOnEachFile>Yes/No</ExecuteOneEachFile>#operates on SIP folder, or each file
        <arguments>--folder %relativePath% --uuid %fileUUID% </arguments>
        <compileCommand>
            <forceLocal>Yes/No</forceLocal>
            <ExecuteOnEachFile>Yes/No</ExecuteOneEachFile>#operates on SIP folder, or each file
            <command>check if successful</command>
            <arguments>--folder %relativePath%--uuid %fileUUID% </arguments>
        </compileCommand>
        <verificationCommand>
            <forceLocal>Yes/No</forceLocal>
            <ExecuteOnEachFile>Yes/No</ExecuteOneEachFile>#operates on SIP folder, or each file
            <command>check if successful</command>
            <arguments>--folder %relativePath%--uuid %fileUUID% </arguments>
            <folderToMoveSIPToIfUnsuccessful><folderToMoveSIPToIfUnsuccessful>
            <folderToMoveSIPToIfSuccessful><folderToMoveSIPToIfSuccessful>
        </verificationCommand>
    </command>
</module>
