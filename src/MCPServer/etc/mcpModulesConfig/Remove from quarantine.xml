<module>
    <requiresUserApproval>Yes</requiresUserApproval>
    <descriptionForApproval>Unquarantine SIP</descriptionForApproval>
    <notificationStarted></notificationStarted>
    <notificationCompletedWithoutErrors></notificationCompletedWithoutErrors>
    <notificationCompletedWithErrors></notificationCompletedWithErrors>
    
    <directories>
        <watchDirectory>%watchDirectoryPath%quarantined/</watchDirectory>
        <processingDirectory>%processingDirectory%</processingDirectory>
        <successDirectory>%watchDirectoryPath%extractPackages/easyExtract/</successDirectory>
        <failureDirectory>%watchDirectoryPath%failed/</failureDirectory>
    </directories>

    <commands>    
        <exeCommand> 
            <command>
                <skip>No</skip>
                <filterFileEnd></filterFileEnd>
                <filterFileStart></filterFileStart>
                <filterSubDir></filterSubDir>
                <requiresOutputLock>No</requiresOutputLock>
                <standardIn></standardIn>
                <standardOut></standardOut>
                <standardError></standardError>
                <descriptionWhileExecuting>test</descriptionWhileExecuting>
                <failureNotification> </failureNotification>
                <execute>setFilePermission_v0.0</execute>
                <arguments>-R 740 "%relativeLocation%"</arguments>
                <executeOnEachFile>no</executeOnEachFile>
            </command>
        </exeCommand>
        
        <verificationCommand>
            <command>
                <descriptionWhileExecuting> </descriptionWhileExecuting>
                <skip>No</skip>
                <filterFileEnd></filterFileEnd>
                <filterFileStart></filterFileStart>
                <filterSubDir>logs/fileMeta/</filterSubDir>
                <requiresOutputLock>No</requiresOutputLock>
                <standardIn></standardIn>
                <standardOut>%SIPDirectory%ArchivematicaUnquarantineEvent.xml</standardOut>
                <standardError>%SIPDirectory%ArchivematicaUnquarantineEvent.xml</standardError>
                <failureNotification> </failureNotification>
                <execute>createXMLEventUnquarantine_v0.0</execute>
                <arguments>"%taskUUID%" "%jobCreatedDate%" "%relativeLocation%"</arguments>
                <executeOnEachFile>Yes</executeOnEachFile>
            </command>           
        </verificationCommand>
        
        <cleanupSuccessfulCommand>
            <command>
                <descriptionWhileExecuting> </descriptionWhileExecuting>
                <skip>Yes/No</skip>
                <filterFileEnd></filterFileEnd>
                <filterFileStart></filterFileStart>
                <filterSubDir></filterSubDir>
                <requiresOutputLock>No</requiresOutputLock>
                <standardIn></standardIn>
                <standardOut></standardOut>
                <standardError></standardError>
                <failureNotification> </failureNotification>
                <execute> </execute>
                <arguments> </arguments>
                <executeOnEachFile> </executeOnEachFile>
            </command> 
        </cleanupSuccessfulCommand>
        
        <cleanupUnsuccessfulCommand>
            <command>
                <skip>Yes/No</skip>
                <filterFileEnd></filterFileEnd>
                <filterFileStart></filterFileStart>
                <filterSubDir></filterSubDir>
                <requiresOutputLock>No</requiresOutputLock>
                <standardIn></standardIn>
                <standardOut></standardOut>
                <standardError></standardError>
                <descriptionWhileExecuting> </descriptionWhileExecuting>
                <failureNotification> </failureNotification>
                <execute> </execute>
                <arguments> </arguments>
                <executeOnEachFile> </executeOnEachFile>
            </command>              
        </cleanupUnsuccessfulCommand>                  
    </commands>  
</module>
