<module>

    <requiresUserApproval>No</requiresUserApproval>
    <descriptionForApproval>None Needed </descriptionForApproval>
    <notificationStarted></notificationStarted>
    <notificationCompletedWithoutErrors></notificationCompletedWithoutErrors>
    <notificationCompletedWithErrors></notificationCompletedWithErrors>
    
    <directories>
        <watchDirectory>%watchDirectoryPath%extractPackages/easyExtract/</watchDirectory>
        <processingDirectory>%processingDirectory%</processingDirectory>
        <successDirectory>%watchDirectoryPath%sanitizeFileNames/detox/</successDirectory>
        <failureDirectory>%watchDirectoryPath%failed/</failureDirectory>
    </directories>

    <commands>    
        <exeCommand> 
            <command>
                <skip>No</skip>
                <filterFileEnd></filterFileEnd>
                <filterFileStart></filterFileStart>
                <filterSubDir></filterSubDir>
                <standardIn></standardIn>
                <standardOut>%relativeLocation%logs/extraction.log</standardOut>
                <standardError>%relativeLocation%logs/extraction.log</standardError>
                <requiresOutputLock>No</requiresOutputLock>
                <descriptionWhileExecuting>test</descriptionWhileExecuting>
                <failureNotification> </failureNotification>
                <execute>easyExtract_v0.0</execute>
                <arguments>"%relativeLocation%" -w -f -r -n</arguments>
                <executeOnEachFile>No</executeOnEachFile>
            </command>
        </exeCommand>
        
        <verificationCommand>
            <command>
                <descriptionWhileExecuting> </descriptionWhileExecuting>
                <skip>No</skip>
                <filterFileEnd></filterFileEnd>
                <filterFileStart></filterFileStart>
                <filterSubDir>objects</filterSubDir>
                <standardIn></standardIn>
                <standardOut>%SIPLogsDirectory%FileUUIDs.log</standardOut>
                <standardError>%SIPLogsDirectory%FileUUIDsError.log</standardError>
                <requiresOutputLock>No</requiresOutputLock>
                <failureNotification> </failureNotification>
                <execute>UUID4PostExtract_v0.0</execute>
                <arguments>"%SIPObjectsDirectory%" "%SIPLogsDirectory%" "%relativeLocation%" "%fileUUID%" "ingest file - extracted" "%date%" "%taskUUID%"</arguments>
                <executeOnEachFile>yes</executeOnEachFile>
            </command>            
        </verificationCommand>
        
        <cleanupSuccessfulCommand>
            <command>
                <descriptionWhileExecuting> </descriptionWhileExecuting>
                <skip>Yes</skip>
                <filterFileEnd></filterFileEnd>
                <filterFileStart></filterFileStart>
                <filterSubDir></filterSubDir>
                <standardIn></standardIn>
                <standardOut></standardOut>
                <standardError></standardError>
                <requiresOutputLock>No</requiresOutputLock>
                <failureNotification> </failureNotification>
                <execute> </execute>
                <arguments> </arguments>
                <executeOnEachFile> </executeOnEachFile>
            </command>            
        </cleanupSuccessfulCommand>
        
        <cleanupUnsuccessfulCommand>
            <command>
                <skip>Yes</skip>
                <filterFileEnd></filterFileEnd>
                <filterFileStart></filterFileStart>
                <filterSubDir></filterSubDir>
                <standardIn></standardIn>
                <standardOut></standardOut>
                <standardError></standardError>
                <requiresOutputLock>No</requiresOutputLock>
                <descriptionWhileExecuting> </descriptionWhileExecuting>
                <failureNotification> </failureNotification>
                <execute> </execute>
                <arguments> </arguments>
                <executeOnEachFile> </executeOnEachFile>
            </command>              
        </cleanupUnsuccessfulCommand>                  
    </commands>  
</module>
