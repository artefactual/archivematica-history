<module>
    <requiresUserApproval>Yes</requiresUserApproval>
    <descriptionForApproval>If desired, review the DIP and remove any access copies that should not be uploaded to the public access system, e.g. for copyright or security reasons. Click "Approve" to upload the DIP to the access system.</descriptionForApproval>
    <notificationStarted></notificationStarted>
    <notificationCompletedWithoutErrors></notificationCompletedWithoutErrors>
    <notificationCompletedWithErrors></notificationCompletedWithErrors>
    
    <directories>
        <watchDirectory>%watchDirectoryPath%uploadDIP</watchDirectory>
        <processingDirectory>%processingDirectory%</processingDirectory>
        <successDirectory>%watchDirectoryPath%uploadedDIPs</successDirectory>
        <failureDirectory>/%watchDirectoryPath%failed</failureDirectory>
    </directories>

    <commands>    
        <exeCommand> 
            <command>
                <skip>No</skip>
                <filterFileEnd></filterFileEnd>
                <filterFileStart></filterFileStart>
                <filterSubDir></filterSubDir>
                <standardIn></standardIn>
                <standardOut>%SIPDirectory%upload.log</standardOut>
                <standardError>%SIPDirectory%uploadError.log</standardError>
                <requiresOutputLock>No</requiresOutputLock>
                <descriptionWhileExecuting>test</descriptionWhileExecuting>
                <failureNotification> </failureNotification>
                <execute>backupDIP_v0.0</execute>
                <arguments>"%SIPDirectory%" "%sharedPath%DIPbackups/"</arguments>
                <executeOnEachFile>No</executeOnEachFile>
            </command>
        </exeCommand>
        
        <verificationCommand>
            <command>
                <skip>No</skip>
                <filterFileEnd></filterFileEnd>
                <filterFileStart></filterFileStart>
                <filterSubDir></filterSubDir>
                <standardIn></standardIn>
                <standardOut></standardOut>
                <standardError>%SIPDirectory%uploadError.log</standardError>
                <requiresOutputLock>No</requiresOutputLock>
                <descriptionWhileExecuting>test</descriptionWhileExecuting>
                <failureNotification> </failureNotification>
                <execute>upload-qubit_v0.0</execute>
                <arguments>-r -e demo@example.com -p demo -t "%SIPName%-%SIPUUID%" -f "%SIPDirectory%" </arguments>
                <executeOnEachFile>No</executeOnEachFile>
            </command>            
        </verificationCommand>
        
        <cleanupSuccessfulCommand>
            <command>
                <descriptionWhileExecuting> </descriptionWhileExecuting>
                <skip>Yes/No</skip>
                <filterFileEnd></filterFileEnd>
                <filterFileStart></filterFileStart>
                <filterSubDir></filterSubDir>
                <standardIn></standardIn>
                <standardOut></standardOut>
                <standardError></standardError>
                <requiresOutputLock>No</requiresOutputLock>
                <failureNotification> </failureNotification>
                <execute> </execute>
                <arguments> </arguments>
                <executeOnEachFile> </executeOnEachFile>
            </command>            
        </cleanupSuccessfulCommand>
        
        <cleanupUnsuccessfulCommand>
            <command>
                <skip>Yes/No</skip>
                <filterFileEnd></filterFileEnd>
                <filterFileStart></filterFileStart>
                <filterSubDir></filterSubDir>
                <standardIn></standardIn>
                <standardOut></standardOut>
                <standardError></standardError>
                <requiresOutputLock>No</requiresOutputLock>
                <descriptionWhileExecuting> </descriptionWhileExecuting>
                <failureNotification> </failureNotification>
                <execute> </execute>
                <arguments> </arguments>
                <executeOnEachFile> </executeOnEachFile>
            </command>              
        </cleanupUnsuccessfulCommand>                  
    </commands>  
</module>
